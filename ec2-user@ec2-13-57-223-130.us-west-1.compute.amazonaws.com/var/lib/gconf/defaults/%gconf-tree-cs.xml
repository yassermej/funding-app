<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="alarm-clock">
				<dir name="alarm">
					<entry name="command">
						<local_schema short_desc="Příkaz">
						</local_schema>
					</entry>
					<entry name="sound_repeat">
						<local_schema short_desc="Opakovat zvuk">
							<longdesc>Jestli opakovat zvuk budíku.</longdesc>
						</local_schema>
					</entry>
					<entry name="sound_file">
						<local_schema short_desc="Zvukový soubor">
						</local_schema>
					</entry>
					<entry name="notify_type">
						<local_schema short_desc="Typ upozornění">
						</local_schema>
					</entry>
					<entry name="repeat">
						<local_schema short_desc="Opakování budíku">
							<longdesc>Seznam dnů kdy se má alarm opakovat.Toto nastavení je pouze pro alarmy v režimu hodin.</longdesc>
						</local_schema>
					</entry>
					<entry name="message">
						<local_schema short_desc="Zpráva budíku">
							<longdesc>Zpráva popisující alarm.</longdesc>
						</local_schema>
					</entry>
					<entry name="time">
						<local_schema short_desc="Čas budíku">
						</local_schema>
					</entry>
					<entry name="type">
						<local_schema short_desc="Typ budíku">
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
